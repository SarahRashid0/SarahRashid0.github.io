{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": {
    "text": "Public Sector Employment in the UK",
    "font": "Baskerville"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/SarahRashid0/SarahRashid0.github.io/refs/heads/main/portfolio_task2/public_sector_employment.csv"
  },

  "height": 400,
  "width":400,

  "mark": {
    "type": "line",
    "color": "navy"
  },

  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "axis": {
        "title": "",
        "grid": false,
        "labelFont": "Baskerville",
        "titleFont": "Baskerville"
      }
    },
    "y": {
      "field": "Value",
      "type": "quantitative",
      "axis": {
        "title": "",
        "grid": false,
        "labelFont": "Baskerville",
        "titleFont": "Baskerville"
      }
    }
  },
  "config": {
    "style": {
      "guide-label": {"font": "Baskerville"},
      "guide-title": {"font": "Baskerville"}
    }
  }
}